@inject IViewLocalizer Localizer

@{
    ViewData["Title"] = Localizer["Create upload file"];
}

<h2 class="pb-2 mb-4 border-bottom">@ViewData["Title"]</h2>

<form method="post" enctype="multipart/form-data" role="form" asp-controller="UploadFiles" asp-action="Create">
    <partial name="_Form" />
    <div class="form-group row mainbtn-area">
        <div class="col-sm-12">
            <div class="float-right">
                <input type="submit" value="@Localizer["Create"]" class="btn btn-primary" />
            </div>
        </div>
    </div>
</form>
